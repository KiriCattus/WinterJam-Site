{
	"name": "winterjam-site",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently  -k --hide 0 \"pnpm:proxy\" \"pnpm:vite\"",
		"vite": "vite dev",
		"proxy": "cf-bindings-proxy",
		"build": "vite build",
		"preview": "vite preview",
