{
	"name": "winterjam-site",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently  -k --hide 0 \"npm:proxy\" \"npm:vite\"",
		"vite": "vite dev",
		"proxy": "cf-bindings-proxy",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251128.0",
		"@sveltejs/adapter-cloudflare": "^3.0.0",
		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"autoprefixer": "^10.4.14",
		"cf-bindings-proxy": "^0.7.2",
		"concurrently": "^9.0.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.24",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^5.45.2",
		"svelte-check": "^4.0.0",
		"svelte-meta-tags": "^4.5.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^7.2.4",
		"vite-plugin-devtools-json": "^1.0.0",
		"wrangler": "^4.51.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/press-start-2p": "^5.0.17",
		"@tsparticles/engine": "^3.9.1",
		"@tsparticles/slim": "^3.9.1",
		"@tsparticles/svelte": "^3.1.1",
		"classnames": "^2.3.2",
		"flowbite": "^3.0.0",
		"flowbite-svelte": "^0.47.0",
		"flowbite-svelte-icons": "^1.0.0",
		"human-format": "^1.2.0"
	}
}
