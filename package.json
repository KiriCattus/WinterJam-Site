		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22.19.1",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"cf-bindings-proxy": "^0.7.2",
		"concurrently": "^9.0.0",
		"eslint": "^9.39.1",
